1 TYPELIB "versioninfo.rc"

#include <winver.h>

#ifdef NDEBUG
    #define DEBUG_FILE_FLAG 0
#else
    #define DEBUG_FILE_FLAG VS_FF_DEBUG
#endif
#if @PROJECT_VERSION_MAJOR@ == 0
    #define PRERELEASE_FILE_FLAG VS_FF_PRERELEASE
#else
    #define PRERELEASE_FILE_FLAG 0
#endif

1 VERSIONINFO
FILEVERSION     @PROJECT_VERSION_MAJOR@, @PROJECT_VERSION_MINOR@, 0, 0
PRODUCTVERSION  @PROJECT_VERSION_MAJOR@, @PROJECT_VERSION_MINOR@, 0, 0
FILEFLAGSMASK   VS_FFI_FILEFLAGSMASK
FILEFLAGS       (DEBUG_FILE_FLAG | PRERELEASE_FILE_FLAG)
FILEOS          VOS_NT_WINDOWS32
FILETYPE        VFT_DLL
FILESUBTYPE     0
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904e4"
        BEGIN
            VALUE "FileVersion","@PROJECT_VERSION_MAJOR@, @PROJECT_VERSION_MINOR@, 0, 0"
            VALUE "InternalName", "@PROJECT_NAME@"
            VALUE "ProductName", "@PROJECT_NAME@"
            VALUE "ProductVersion","@PROJECT_VERSION_PREFIX@@PROJECT_VERSION_MAJOR@.@PROJECT_VERSION_MINOR@"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1252
    END
END
